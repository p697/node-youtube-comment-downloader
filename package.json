{
  "name": "youtube-comment-downloader",
  "version": "0.1.1",
  "description": "Simple script for downloading Youtube comments without using the Youtube API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "youtube-comment-downloader": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:run": "tsx scripts/test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "youtube",
    "comments",
    "downloader",
    "youtube-comments",
    "scraper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chrono-node": "^2.7.0",
    "commander": "^11.0.0",
    "https-proxy-agent": "^7.0.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.5",
    "typescript": "^5.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/p697/node-youtube-comment-downloader.git"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
